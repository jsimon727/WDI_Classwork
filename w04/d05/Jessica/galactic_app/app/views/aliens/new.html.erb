<h1>Create a new Alien</h1>
<form action="/spaceships/<%= params[:spaceship_id] %>/aliens/" method="POST">
  <input name="authenticity_token" type="hidden" value="<%=form_authenticity_token%>">
  
  <label>
      Name:
      <input type="string" name="name">
  </label>

  <label>
      Appendages:
      <input type="integer" name="appendages">
  </label>

   <label>
      Origin:
      <input type="string" name="origin">
  </label>

  <label>
      Organic Substrate:
      <input type="string" name="organic_substrate">
  </label>

  <label>
        Weakness:
      <input type= "string" name="weakness">
  </label>

    <label>
        Strength:
      <input type= "string" name="strength">
  </label>
  
  <button>Create Alien</button>

</form>